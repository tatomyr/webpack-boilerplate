{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/tatomyr/webpack-boilerplate.git"
  },
  "engines": {
    "node": "8.1.2",
    "npm": "5.4.2"
  },
  "scripts": {
    "build": "bash tools/copy-assets.sh && webpack -p",
    "dev": "bash tools/copy-assets.sh && webpack -d --watch",
    "serve": "bash tools/copy-assets.sh && webpack-dev-server",
    "server": "bash tools/copy-assets.sh && webpack -p --config webpack.server-config.babel.js && cd dist/ && node server.bundle.js",
    "deploy:heroku": "bash tools/deployment.sh",
    "clear": "bash tools/clear.sh",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "css-loader": "^0.28.7",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "express": "^4.15.5",
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  }
